{
  "name": "webpackproject",
  "version": "1.0.0",
  "description": "Curso de webpack con platzi",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build-local": "webpack --mode production",
    "build-external": "webpack --config ./external/webpack.config.js",
    "build-multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "build-css": "webpack --config ./css-style-loader/webpack.config.js",
    "build-css-plugin": "webpack --config ./plugins/webpack.config.js",
    "build-dev-server": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build-babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build-react": "webpack-dev-server --config ./react/webpack.config.js",
    "build-url-loader": "webpack-dev-server --config ./url-loader/webpack.config.js",
    "build-prepro": "webpack-dev-server --config ./prepro/webpack.config.js",
    "build-prevent-duplication": "webpack --config ./prevent-duplication/webpack.config.js",
    "build-dll": "webpack --config ./dll/webpack.dll.config.js",
    "build-prevent-duplication-optimizado": "webpack --config ./dll/webpack.config.js",
    "build-dynamic-dll": "webpack --config ./dynamic-imports/webpack.dll.config.js",
    "build-dynamic": "webpack --config ./dynamic-imports/webpack.config.js",
    "_comment": "dll significa --> dynamic-link-library, esta linea es un comentario para recordar"
  },
  "keywords": [
    "webpack",
    "project"
  ],
  "author": "@manuelmelendez0",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.11.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-loader": "8.1.0",
    "css-loader": "2.1.1",
    "file-loader": "6.1.0",
    "html-webpack-plugin": "4.4.1",
    "mini-css-extract-plugin": "0.11.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.3",
    "sass-loader": "10.0.2",
    "style-loader": "0.23.1",
    "url-loader": "4.1.0",
    "webpack": "4.32.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  }
}
